{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run --config vitest.e2e.config.ts test/e2e/api-integration.e2e.test.ts",
    "test:e2e:all": "vitest run --config vitest.e2e.config.ts",
    "test:e2e:watch": "vitest --config vitest.e2e.config.ts",
    "cy:open": "cypress open",
    "test:cy": "cypress run --browser electron",
    "test:cy:headless": "cypress run --browser electron --headless",
    "test:cy:coverage": "cypress run --browser electron --headless && nyc report --reporter=html",
    "test:all": "npm run test:coverage && npm run test:e2e"
  },
  "dependencies": {
    "@aws-amplify/ui-vue": "^4.3.6",
    "@tailwindcss/vite": "^4.1.12",
    "aws-amplify": "^6.15.7",
    "axios": "^1.12.1",
    "nuxt": "^4.0.3",
    "tailwindcss": "^4.1.12",
    "vue": "^3.5.20",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@nuxt/test-utils": "^3.20.1",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/user-event": "14.5.2",
    "@testing-library/vue": "8.1.0",
    "@types/node": "^24.8.0",
    "@vitejs/plugin-vue": "6.0.1",
    "@vitest/coverage-v8": "3.2.0",
    "@vitest/ui": "3.2.0",
    "@vue/test-utils": "2.4.6",
    "cypress": "13.17.0",
    "happy-dom": "^20.0.10",
    "jsdom": "25.0.1",
    "nyc": "17.1.0",
    "sass-embedded": "^1.93.3",
    "typescript": "^5.9.3",
    "vitest": "3.2.0"
  },
  "version": "1.0.0"
}
